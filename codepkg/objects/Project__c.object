    <validationRules>
        <fullName>Admin_Share_Validation_for_CDAA</fullName>
        <active>false</active>
        <description>For Non - CDAA Programs State Admin% should always be Zero.</description>
        <errorConditionFormula>AND( NOT( CONTAINS(  Program_Name__r.Master_Label_Program_Name__c  , &apos;CDAA&apos;) ) , StateAdmin__c != 0)</errorConditionFormula>
        <errorDisplayField>StateAdmin__c</errorDisplayField>
        <errorMessage>For Non - CDAA Programs State Admin% should always be Zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Declaration_Number_Field_Validation</fullName>
        <active>true</active>
        <description>Declaration Number is Required at Stage &quot;Obligating&quot;, If the Incident number field is not empty.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ), 
AND( 
 OR(ISPICKVAL( Stage__c , &apos;Obligating&apos;),ISPICKVAL( Stage__c , &apos;Compliance Verification&apos;) ,ISPICKVAL( Stage__c , &apos;Active Payment&apos;), ISPICKVAL( Stage__c , &apos;Deobligation&apos;) ) , NOT( ISBLANK( Incident_Name__c ) ),  ISBLANK( Declaration_Number__c )  ))</errorConditionFormula>
        <errorDisplayField>Incident_Name__c</errorDisplayField>
        <errorMessage>Please provide &quot;Declaration Number&quot; for the Incident.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Eligible_for_State_Admin_Validation</fullName>
        <active>false</active>
        <description>Eligible for State Admin is applicable only for CDAA Program.</description>
        <errorConditionFormula>AND( NOT( CONTAINS(Program_Name__r.Name, &apos;CDAA&apos;)) ,
OR( ISPICKVAL( EligibleforStateAdminShare__c , &apos;Yes&apos;) ,
ISPICKVAL( PaytoStateAdmin__c , &apos;Yes&apos;) ) )</errorConditionFormula>
        <errorMessage>Eligible for State Admin is applicable only for CDAA Program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Required</fullName>
        <active>true</active>
        <description>Please enter &quot;End Date&quot; for the Project.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,
AND(OR(ISPICKVAL( Stage__c , &apos;Obligating&apos;),ISPICKVAL( Stage__c , &apos;Compliance Verification&apos;) ,ISPICKVAL( Stage__c , &apos;Active Payment&apos;), ISPICKVAL( Stage__c , &apos;Deobligation&apos;) ) ,ISBLANK( End_Date__c)  ))</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Please provide &quot;End Date&quot; for the Project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FEMA_Share_Validation</fullName>
        <active>false</active>
        <description>FEMA Share % is mandatory if Stage is Obligating and Funding Source is Federal and State or Federal only</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ), 
AND(OR(ISPICKVAL( Stage__c , &apos;Obligating&apos;),ISPICKVAL( Stage__c , &apos;Compliance Verification&apos;) ,ISPICKVAL( Stage__c , &apos;Active Payment&apos;), ISPICKVAL( Stage__c , &apos;Deobligation&apos;) ), OR( ISPICKVAL( FundingSource__c,&apos;Federal and State&apos;),ISPICKVAL( FundingSource__c,&apos;Federal Only&apos;) ), OR(ISBLANK( FEMA_Share__c ),FEMA_Share__c ==0) ))</errorConditionFormula>
        <errorDisplayField>FEMA_Share__c</errorDisplayField>
        <errorMessage>Please provide &quot;FEMA Share % &quot; for the Project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Federal_Project_Threshold_Validation</fullName>
        <active>true</active>
        <description>When Funding Source is &quot;Federal and State&quot; or &quot;State Only&quot;, then Federal Small Project Threshold is required.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ), 
AND( OR(ISPICKVAL( Stage__c , &apos;Obligating&apos;),ISPICKVAL( Stage__c , &apos;Compliance Verification&apos;) ,ISPICKVAL( Stage__c , &apos;Active Payment&apos;), ISPICKVAL( Stage__c , &apos;Deobligation&apos;) ) , NOT( ISBLANK( Incident_Name__c ) ), OR(ISPICKVAL( FundingSource__c , &apos;Federal and State&apos;),ISPICKVAL( FundingSource__c , &apos;Federal Only&apos;)) , ISBLANK( Incident_Name__r.Federal_Small_Project_Threshold__c ) ))</errorConditionFormula>
        <errorDisplayField>Incident_Name__c</errorDisplayField>
        <errorMessage>Please provide &quot;Federal Small Project Threshold&quot; for the Incident.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Federal_Retention_Validation</fullName>
        <active>true</active>
        <description>&quot;Federal Retention&quot; is not applicable for &quot;State Only&quot; Projects.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ),
AND( ISPICKVAL( FundingSource__c , &apos;State Only&apos;) , AND(NOT( ISBLANK(  Federal_Retention__c ) ), Federal_Retention__c != 0)))</errorConditionFormula>
        <errorDisplayField>Federal_Retention__c</errorDisplayField>
        <errorMessage>&quot;Federal Retention %&quot; is not applicable for &quot;State Only&quot; Projects.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Federal_Share_Validation</fullName>
        <active>true</active>
        <description>Federal Share is not applicable for State Only Projects.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ),
