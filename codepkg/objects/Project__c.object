    <fields>
        <fullName>Email_Template_NSPO__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND(FIPS_ID__c  !=null , CalOES_Project_Number__c !=null,  CalOES_ID__c !=null),
   Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ FIPS_ID__c +&quot;/&quot;+ CalOES_Project_Number__c +&quot;/&quot;+ CalOES_ID__c,
   
   IF( AND(FIPS_ID__c  !=null , CalOES_Project_Number__c !=null,  CalOES_ID__c ==null),
   Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ FIPS_ID__c +&quot;/&quot;+ CalOES_Project_Number__c , 
   
		IF( AND(FIPS_ID__c  !=null , CalOES_Project_Number__c ==null,  CalOES_ID__c !=null),
		Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ FIPS_ID__c +&quot;/&quot;+ CalOES_ID__c , 
		
		IF( AND(FIPS_ID__c  ==null , CalOES_Project_Number__c !=null,  CalOES_ID__c !=null),
		Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ CalOES_Project_Number__c +&quot;/&quot;+ CalOES_ID__c,
		
		IF( AND(FIPS_ID__c  !=null , CalOES_Project_Number__c ==null,  CalOES_ID__c ==null),
        Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ FIPS_ID__c,
		
		IF( AND(FIPS_ID__c  ==null , CalOES_Project_Number__c !=null,  CalOES_ID__c ==null),
        Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ CalOES_Project_Number__c, 
		
		IF( AND(FIPS_ID__c  ==null , CalOES_Project_Number__c ==null,  CalOES_ID__c !=null),
        Obligated_Sub_Recipient_Formula__c+&quot;/&quot;+ CalOES_ID__c, Obligated_Sub_Recipient_Formula__c)
         ))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Email Template NSPO</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
