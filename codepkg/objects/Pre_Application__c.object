false)</errorConditionFormula>
        <errorDisplayField>Disaster__c</errorDisplayField>
        <errorMessage>Please Select Disaster</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Approve_NOI_Without_Due_Date</fullName>
        <active>true</active>
        <description>Restrict Approve the NOI Without sub-Application Due Date</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;HMGP&apos;,
					 IF( 
				OR(
								AND(ISPICKVAL(Status__c, &apos;Approved&apos;), ISPICKVAL(Status_Reason__c,&apos;NOI Approved&apos;), ISBLANK(Sub_Application_Due_Date__c) ),  
							 	   AND($UserRole.Name &lt;&gt; &apos;HMA Manager&apos;,  ISCHANGED(Sub_Application_Due_Date__c) ) 
				       ) , 
			    true, false)
				)</errorConditionFormula>
        <errorDisplayField>Sub_Application_Due_Date__c</errorDisplayField>
        <errorMessage>Only HMA Manager can update the Sub-Application Due Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Approve_Or_Reject</fullName>
        <active>true</active>
        <description>If Is Project Phased field value not set</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;HMGP&apos;,
				IF(
				AND(
								OR(
												ISPICKVAL(Status__c, &apos;Approved&apos;),
												ISPICKVAL(Status__c, &apos;Rejected&apos;)
								),
								ISBLANK( TEXT(Is_this_project_phased__c))
			  	 ),
				   true, false)
			)</errorConditionFormula>
        <errorDisplayField>Is_this_project_phased__c</errorDisplayField>
        <errorMessage>Please Indicate whether this NOI will result in a phased Project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Owner_Change</fullName>
        <active>true</active>
        <description>Restrict HMA Analyst User to change the Owner</description>
        <errorConditionFormula>$User.Title = &quot;HMA Analyst&quot; &amp;&amp; ISCHANGED( OwnerId )</errorConditionFormula>
        <errorMessage>Only Managers can change the owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Status_Change</fullName>
        <active>true</active>
        <description>Restrict status change to Approved or Rejected by HMA Analyst</description>
        <errorConditionFormula>AND( $User.Title = &quot;HMA Analyst&quot;, OR( ISPICKVAL(Status__c, &apos;Approved&apos;), ISPICKVAL(Status__c, &apos;Rejected&apos;)))</errorConditionFormula>
        <errorMessage>Only managers can approve or reject an NOI</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
