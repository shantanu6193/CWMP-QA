    <validationRules>
        <fullName>Actual_End_Date_Start_Date_Validation</fullName>
        <active>true</active>
        <description>Actual End Date should be greater than Actual Start Date</description>
        <errorConditionFormula>AND( ISBLANK(External_Id__c ),Actual_End_Date__c &lt; Actual_Start_Date__c)</errorConditionFormula>
        <errorDisplayField>Actual_End_Date__c</errorDisplayField>
        <errorMessage>&quot;Actual End Date&quot; should be greater than &quot;Actual Start Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Planned_Start_Date_End_Date_validation</fullName>
        <active>true</active>
        <description>Planned End Date should be greater than Planned Start Date</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ) ,Planned_End_Date__c &lt; Planned_Start_Date__c)</errorConditionFormula>
        <errorDisplayField>Planned_End_Date__c</errorDisplayField>
        <errorMessage>&quot;Planned End Date&quot; should be greater than &quot;Planned Start Date&quot;.</errorMessage>
    </validationRules>
