    <validationRules>
        <fullName>Is_BCR_verified_at_1_or_greater_Not_Null</fullName>
        <active>true</active>
        <description>When Application Status=&quot;Withdrawn&quot; &amp; Status Reason=&quot;by OES&quot; then &quot;	
Does project have a verified BCR&quot; is not empty</description>
        <errorConditionFormula>IF( AND(ISPICKVAL(Status__c, &apos;Withdrawn&apos;), ISPICKVAL(Status_Reason__c, &apos;By OES&apos;)),
    ISPICKVAL(Is_BCR_verified_at_1_0_or_greater__c, &apos;&apos;), false)</errorConditionFormula>
        <errorDisplayField>Is_BCR_verified_at_1_0_or_greater__c</errorDisplayField>
        <errorMessage>Please complete this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_BCR_verified_at_1_or_greater_validate</fullName>
        <active>true</active>
        <description>When &apos;Verified Benefit Cost Ratio After Review &lt; 1&apos; then &apos;BCR 1.0 or greater?&apos; must be No 
And When &apos;Verified Benefit Cost Ratio After Review &gt;= 1&apos; then &apos;BCR 1.0 or greater?&apos; must be Yes 
And for &apos;null/blank&apos; it must be &apos;pending or not applicable&apos;</description>
        <errorConditionFormula>AND(  IF(  NOT(ISPICKVAL( Status__c , &apos;Draft&apos;)), true, false),
				  OR(IF($UserRole.Name = &apos;HMA Manager&apos;, true, false), IF($UserRole.Name = &apos;HMA Analyst&apos;, true, false)),
					 OR( IF( ABS(Verified_Benefit_Cost_Ratio_After_Review__c) &lt; 1, NOT(ISPICKVAL(Is_BCR_verified_at_1_0_or_greater__c, &apos;No&apos;)), false) ,
				IF( ABS(Verified_Benefit_Cost_Ratio_After_Review__c)  &gt;= 1, NOT(ISPICKVAL(Is_BCR_verified_at_1_0_or_greater__c, &apos;Yes&apos;)), false) ,
				IF( OR( ISNULL(ABS(Verified_Benefit_Cost_Ratio_After_Review__c)),  
											 ISBLANK(ABS(Verified_Benefit_Cost_Ratio_After_Review__c))
										) , 
							 NOT( OR(ISPICKVAL(Is_BCR_verified_at_1_0_or_greater__c, &apos;Pending&apos;),
																ISPICKVAL(Is_BCR_verified_at_1_0_or_greater__c, &apos;Not Applicable&apos;)
															)
											), false
						)
		     )
		)</errorConditionFormula>
        <errorDisplayField>Is_BCR_verified_at_1_0_or_greater__c</errorDisplayField>
        <errorMessage>When &apos;Verified Benefit Cost Ratio After Review &lt; 1&apos; then &apos;BCR 1.0 or greater?&apos; must be &quot;No&quot;
OR When &apos;Verified Benefit Cost Ratio After Review &gt;= 1&apos; then &apos;BCR 1.0 or greater?&apos; must be &quot;Yes&quot;
OR for &apos;null/blank&apos; it must be &quot;pending&quot; or &quot;not applicable&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_this_a_eligible_Subapplicant_Not_null</fullName>
        <active>true</active>
        <description>When Application Status=&quot;Withdrawn&quot; &amp; Status Reason=&quot;by OES&quot; then &quot;Is this an eligible Subapplicant?&quot; is not empty</description>
        <errorConditionFormula>IF( AND(ISPICKVAL(Status__c, &apos;Withdrawn&apos;), ISPICKVAL(Status_Reason__c, &apos;By OES&apos;)),
    ISPICKVAL(Is_this_an_eligible_Subapplicant__c, &apos;&apos;), false)</errorConditionFormula>
        <errorDisplayField>Is_this_an_eligible_Subapplicant__c</errorDisplayField>
        <errorMessage>Please complete this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_this_an_eligible_Activity_Not_Null</fullName>
        <active>true</active>
        <description>When Application Status=&quot;Withdrawn&quot; &amp; Status Reson=&quot;by OES&quot; then &quot;Is this an eligible Activity?&quot; is not empty</description>
        <errorConditionFormula>IF( AND(ISPICKVAL(Status__c, &apos;Withdrawn&apos;), ISPICKVAL(Status_Reason__c, &apos;By OES&apos;)),
ISPICKVAL(Is_this_an_eligible_Activity__c, &apos;&apos;), false)</errorConditionFormula>
        <errorDisplayField>Is_this_an_eligible_Activity__c</errorDisplayField>
        <errorMessage>Please complete this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Local_Hazard_Mitigation_Status_Not_Null</fullName>
        <active>true</active>
        <description>When Application Status=&quot;Withdrawn&quot; &amp; Status Reson=&quot;by OES&quot; then &quot;Local Hazard Mitigation Plan Status&quot; is not empty</description>
        <errorConditionFormula>IF( AND(ISPICKVAL(Status__c, &apos;Withdrawn&apos;), ISPICKVAL(Status_Reason__c, &apos;By OES&apos;)), 
		       ISPICKVAL(Local_Hazard_Mitigation_Plan_Status__c, &apos;&apos;), false)</errorConditionFormula>
        <errorDisplayField>Local_Hazard_Mitigation_Plan_Status__c</errorDisplayField>
        <errorMessage>Please complete this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Program_Not_null</fullName>
        <active>true</active>
        <description>If Has_subapp_been_submitte_to_OES_before__c OR Funded_previously_under_a_Fed_program__c is Yes then Program Should not be null</description>
        <errorConditionFormula>IF( 
						OR(  ISPICKVAL(Has_subapp_been_submitte_to_OES_before__c, &apos;Yes&apos;), 
									ISPICKVAL(Funded_previously_under_a_Fed_program__c, &apos;Yes&apos;)),
  						 IF(OR(ISNULL(Program__c), ISBLANK(Program__c)) , true, false) , false)</errorConditionFormula>
        <errorMessage>Please select the Program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_is_mandatory</fullName>
        <active>true</active>
        <description>Reason is mandatory if ready to submit is no.</description>
        <errorConditionFormula>IF( OR($UserRole.Name = &apos;HMA Manager&apos;,$UserRole.Name = &apos;HMA Analyst&apos;) , 
				    IF( ISPICKVAL( Subapplication_Complete__c , &apos;No&apos;) ,  ISBLANK(Reason__c) , false) , 
				    false)</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Please complete this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Owner_Change</fullName>
        <active>true</active>
        <description>Restrict HMA Analyst users to change the owner.</description>
        <errorConditionFormula>$User.Title = &quot;HMA Analyst&quot; &amp;&amp; ISCHANGED(OwnerId)</errorConditionFormula>
        <errorMessage>Only Managers can change the owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Status_change_to_FEMA_Review</fullName>
        <active>true</active>
        <description>Status can be changed to FEMA Review only when  &apos;Yes&apos; value for Ready to Submit field.</description>
        <errorConditionFormula>IF(AND(OR( $UserRole.Name = &apos;HMA Manager&apos;, $UserRole.Name = &apos;HMA Analyst&apos;),
ISCHANGED(Status__c),
ISPICKVAL(Status__c, &apos;FEMA Review&apos;),
ISPICKVAL(Subapplication_Complete__c, &apos;No&apos;)),
true,
false)</errorConditionFormula>
        <errorDisplayField>Subapplication_Complete__c</errorDisplayField>
        <errorMessage>Only &apos;Yes&apos; value to this field can move the status to &apos;FEMA Review&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Subapplication_comment_change</fullName>
        <active>true</active>
        <description>Restrict user to select the field option when status changes to &quot;OES Decision&quot;</description>
        <errorConditionFormula>IF(AND(OR( $UserRole.Name = &apos;HMA Manager&apos;, $UserRole.Name = &apos;HMA Analyst&apos;),
								 ISCHANGED(Status__c),  
							ISPICKVAL(Status__c, &apos;OES Decision&apos;), 
							ISPICKVAL(Subapplication_Complete__c, &apos;&apos;) ), 
				true, 
				false)</errorConditionFormula>
        <errorDisplayField>Subapplication_Complete__c</errorDisplayField>
        <errorMessage>Please complete this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_withdraw_if_SubApp_met_criteria</fullName>
        <active>true</active>
        <description>Restrict withdraw if Sub-Application met the FEMA submission criteria</description>
        <errorConditionFormula>IF( AND(
          ISPICKVAL(Status__c, &apos;Withdrawn&apos;), 
          ISPICKVAL(Status_Reason__c, &apos;By OES&apos;)
        ), IF(  AND(
                    ISPICKVAL( Is_this_an_eligible_Activity__c , &apos;Yes&apos;),
                    ISPICKVAL( Is_this_an_eligible_Subapplicant__c, &apos;Yes&apos;),
                    ISPICKVAL( Is_BCR_verified_at_1_0_or_greater__c, &apos;Yes&apos;),
                    NOT(ISPICKVAL( Local_Hazard_Mitigation_Plan_Status__c, &apos;Not Started&apos;)) 
                   ), true, false
              ),
            false
    )</errorConditionFormula>
        <errorMessage>Sub-app meets FEMA submission criteria and cannot be withdrawn.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
