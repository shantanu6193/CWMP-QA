    <validationRules>
        <fullName>CWMP_RFI_Prior_Date_Restriction</fullName>
        <active>true</active>
        <description>Restricting CWMP Application RFI from Entering Prior Date in RFI Deadline.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = &apos;CWMP&apos;,  RFI_Deadline__c &lt; TODAY() )</errorConditionFormula>
        <errorMessage>RFI Deadline must not be prior to today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_RFI_Type_only_in_draft_stage</fullName>
        <active>true</active>
        <description>RFI type can only be changed if status is draft</description>
        <errorConditionFormula>OR( $UserRole.Name = &apos;HMA Analyst&apos;, $UserRole.Name = &apos;HMA Manager&apos;)  &amp;&amp;
NOT(ISPICKVAL(Status__c, &apos;Draft&apos;)) &amp;&amp;  ISCHANGED(RFI_Type__c)</errorConditionFormula>
        <errorDisplayField>RFI_Type__c</errorDisplayField>
        <errorMessage>You cannot change RFI Type after the RFI is sent to subapplicant.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FEMA_User_create_FEMA_RFI_Type</fullName>
        <active>false</active>
        <description>Only Fema user should be able to create FEMA related RFI type</description>
        <errorConditionFormula>($UserRole.Name = &quot;HMA Analyst&quot; || $UserRole.Name = &quot;HMA Manager&quot;) &amp;&amp;  (ISPICKVAL( RFI_Type__c , &apos;FEMA OES Programmatic&apos;) ||  ISPICKVAL(RFI_Type__c, &apos;FEMA OES EHP&apos;))</errorConditionFormula>
        <errorDisplayField>RFI_Type__c</errorDisplayField>
        <errorMessage>You are not allowed to create FEMA OES EHP,  FEMA OES Programmatic RFI Types</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Subapplicant_Comment_Change</fullName>
        <active>true</active>
        <description>Analyst and Manger cannot change the subapplicant comments.</description>
        <errorConditionFormula>OR( $UserRole.Name = &apos;HMA Analyst&apos;, $UserRole.Name = &apos;HMA Manager&apos;) &amp;&amp;
ISCHANGED( Subapplicant_Comment__c )</errorConditionFormula>
        <errorDisplayField>Subapplicant_Comment__c</errorDisplayField>
        <errorMessage>You do not have access to change Subapplicant comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Time_Ext_Rej_or_Accp_If_Not_Req</fullName>
        <active>true</active>
        <description>Restrict Time extension Accepted and Rejected if Prior status not equal to Time extension Requested</description>
        <errorConditionFormula>OR( $UserRole.Name = &apos;HMA Analyst&apos;, $UserRole.Name = &apos;HMA Manager&apos;) &amp;&amp;   
	NOT(ISPICKVAL( PRIORVALUE(Status__c) , &apos;Time Extension Requested&apos;) ) &amp;&amp;  
 OR( ISPICKVAL(Status__c, &apos;Time Extension Accepted&apos;) ,  ISPICKVAL(Status__c, &apos;Time Extension Rejected&apos;) )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Prior status should be Time Extension Requested</errorMessage>
    </validationRules>
