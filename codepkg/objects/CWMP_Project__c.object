    <validationRules>
        <fullName>Assessor_Email_Validation</fullName>
        <active>true</active>
        <description>Assessor Email is required before moving the Project to Post Assessment Stage.</description>
        <errorConditionFormula>AND(  OR( AND(ISPICKVAL( Project_Stage__c , &apos;Post-Assessment&apos;), OR(ISPICKVAL(  Project_Status__c , &apos;Post Assessment Pending&apos;), ISPICKVAL( Project_Status__c , &apos;Post Assessment is Completed&apos;))),ISPICKVAL( Project_Stage__c , &apos;Project Complete&apos;)) , ISBLANK( Assessor_Email__c ) )</errorConditionFormula>
        <errorMessage>Please complete &apos;Assessor Email&apos; prior to updating the Project Stage to ‘Post-Assessment’ and Project Status to &apos;Post Assessment Pending&apos;.</errorMessage>
    </validationRules>
