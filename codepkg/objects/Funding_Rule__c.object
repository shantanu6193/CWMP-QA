    <validationRules>
        <fullName>Cannot_Enter_Date_when_type_is_percent</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Rule_Type__c) == &apos;Percentage Basis&apos; &amp;&amp; (
 NOT(ISBLANK( Funding_Source_1_Start_Date__c)) || 
 NOT(ISBLANK( Funding_Source_2_Start_Date__c)) || 
 NOT(ISBLANK( Funding_Source_3_Start_Date__c)) || 
 NOT(ISBLANK( Funding_Source_4_Start_Date__c)) || 
 NOT(ISBLANK( Funding_Source_5_Start_Date__c)) ||
 NOT(ISBLANK( Funding_Source_1_End_Date__c)) || 
 NOT(ISBLANK( Funding_Source_2_End_Date__c)) || 
 NOT(ISBLANK( Funding_Source_3_End_Date__c)) || 
 NOT(ISBLANK( Funding_Source_4_End_Date__c)) || 
 NOT(ISBLANK( Funding_Source_5_End_Date__c)) 
)</errorConditionFormula>
        <errorMessage>You cannot enter value in date fields when the rule type is percentage basis</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Enter_Percent_when_type_is_time</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Rule_Type__c) == &apos;Time Period Basis&apos; &amp;&amp; (
 NOT(ISBLANK( Funding_Source_1_Percent__c )) || 
 NOT(ISBLANK( Funding_Source_2_Percent__c )) || 
 NOT(ISBLANK( Funding_Source_3_Percent__c )) || 
 NOT(ISBLANK( Funding_Source_4_Percent__c )) || 
 NOT(ISBLANK( Funding_Source_5_Percent__c )) 
)</errorConditionFormula>
        <errorMessage>You cannot enter value in percent fields when the rule type is Time Period Basis</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Text(Rule_Type__c) == &apos;Time Period Basis&apos; &amp;&amp;  
(
(ISBLANK(Funding_Source_1_Start_Date__c) || ISBLANK(Funding_Source_1_End_Date__c)) &amp;&amp; NOT(ISBLANK( Funding_Source_1__c ))|| 
(ISBLANK(Funding_Source_2_Start_Date__c) || ISBLANK(Funding_Source_2_End_Date__c))&amp;&amp; NOT(ISBLANK( Funding_Source_2__c ))|| 
(ISBLANK(Funding_Source_3_Start_Date__c) || ISBLANK(Funding_Source_3_End_Date__c))&amp;&amp; NOT(ISBLANK( Funding_Source_3__c ))|| 
(ISBLANK(Funding_Source_4_Start_Date__c) || ISBLANK(Funding_Source_4_End_Date__c))&amp;&amp; NOT(ISBLANK( Funding_Source_4__c ))|| 
(ISBLANK(Funding_Source_5_Start_Date__c) || ISBLANK(Funding_Source_5_End_Date__c))&amp;&amp; NOT(ISBLANK( Funding_Source_5__c ))
)</errorConditionFormula>
        <errorMessage>Please select the start and end date when the rule type is time period basis</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percent_Cannot_be_more_than_100</fullName>
        <active>true</active>
        <errorConditionFormula>Overall_Percentage__c &gt; 1.0</errorConditionFormula>
        <errorMessage>Sum of Percentages of all funding sources cannot be more than 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percent_Type_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Text(Rule_Type__c) == &apos;Percentage Basis&apos; &amp;&amp;  
(
ISBLANK(Funding_Source_1_Percent__c) &amp;&amp; NOT(ISBLANK( Funding_Source_1__c ))|| 
ISBLANK(Funding_Source_2_Percent__c) &amp;&amp; NOT(ISBLANK( Funding_Source_2__c ))|| 
ISBLANK(Funding_Source_3_Percent__c) &amp;&amp; NOT(ISBLANK( Funding_Source_3__c ))|| 
ISBLANK(Funding_Source_4_Percent__c) &amp;&amp; NOT(ISBLANK( Funding_Source_4__c ))|| 
ISBLANK(Funding_Source_5_Percent__c) &amp;&amp; NOT(ISBLANK( Funding_Source_5__c ))
)</errorConditionFormula>
        <errorMessage>Please select the percentage when the rule type is percentage basis</errorMessage>
    </validationRules>
