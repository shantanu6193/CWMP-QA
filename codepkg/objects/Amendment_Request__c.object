    <validationRules>
        <fullName>AdditionalFundAndCostShare_Validation</fullName>
        <active>true</active>
        <description>&quot;Time Extension Start Date&quot; and &quot;Time Extension End Date&quot; are not applicable for the &quot;Additional Funding and FEMA Percentage Change&quot; Amendment.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) , ISPICKVAL( AmendmentType__c , &apos;Additional Funding and FEMA Percentage Change&apos;) , OR(NOT( ISBLANK( TimeExtensionStartDate__c ) ) , NOT( ISBLANK( TimeExtensionEndDate__c ))))</errorConditionFormula>
        <errorMessage>&quot;Time Extension Start Date&quot; and &quot;Time Extension End Date&quot; are not applicable for the &quot;Additional Funding and FEMA Percentage Change&quot; Amendment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AdditionalFundAndTimeExtensionValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) , ISPICKVAL( AmendmentType__c , &apos;Additional Funding and Time Extension&apos;), OR(AND( NOT( ISBLANK(FEMA_Share__c) ) ,FEMA_Share__c != 0) ,AND( NOT( ISBLANK( State_Share__c ) ),State_Share__c != 0)))</errorConditionFormula>
        <errorMessage>&quot;FEMA Share%&quot; and &quot;State Share%&quot; are not applicable for the &quot;Additional Funding and Time Extension&quot; Amendment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Additional_Funding_Validation</fullName>
        <active>true</active>
        <description>Time Extension Start Date, Time Extension End Date, FEMA Share%, and State Share% are not applicable for the &quot;Additional Funding&quot; Amendment.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) , ISPICKVAL( AmendmentType__c , &apos;Money Extension&apos;) , OR( NOT( ISBLANK( TimeExtensionStartDate__c ) ) , NOT( ISBLANK( TimeExtensionEndDate__c )) , AND( NOT( ISBLANK(FEMA_Share__c) ) ,FEMA_Share__c != 0) ,AND( NOT( ISBLANK( State_Share__c ) ),State_Share__c != 0)))</errorConditionFormula>
        <errorMessage>Time Extension Start Date, Time Extension End Date, FEMA Share%, and State Share% are not applicable for the &quot;Additional Funding&quot; Amendment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amendment_Type_Validation</fullName>
        <active>true</active>
        <description>Amendment Type is Required</description>
        <errorConditionFormula>ISPICKVAL(AmendmentType__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>AmendmentType__c</errorDisplayField>
        <errorMessage>Please provide Amendment Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FEMA_Percentage_Change_Validation</fullName>
        <active>true</active>
        <description>Time Extension Start Date, Time Extension End Date, and Supplement Award are not applicable for the &quot;FEMA PErcentage Change&quot; Amendment.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) , ISPICKVAL( AmendmentType__c , &apos;FEMA Percentage Change&apos;) , OR(  AND( NOT( ISBLANK( SupplementAward__c ) )  ,SupplementAward__c != 0) , NOT( ISBLANK(  TimeExtensionStartDate__c ) ) , NOT( ISBLANK(  TimeExtensionEndDate__c ))))</errorConditionFormula>
        <errorMessage>Time Extension Start Date, Time Extension End Date, and Supplement Award are not applicable for the &quot;FEMA PErcentage Change&quot; Amendment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Obligated_Amount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),AND( ISPICKVAL( AmendmentType__c , &apos;Money Extension&apos;) , ISBLANK( NewObligatedAmount__c )))</errorConditionFormula>
        <errorDisplayField>NewObligatedAmount__c</errorDisplayField>
        <errorMessage>Please provide &quot;New Obligated Amount&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Validation</fullName>
        <active>true</active>
        <description>Please select a Project for Amendment Request.</description>
        <errorConditionFormula>ISBLANK( Project__c )</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>Please select a Project for Amendment Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Time_Extension</fullName>
        <active>false</active>
        <errorConditionFormula>$Permission.PA_Amendment_Request_Restriction = true &amp;&amp;
(NOT(ISPICKVAL( AmendmentType__c ,&quot;Time Extension&quot;)))</errorConditionFormula>
        <errorDisplayField>AmendmentType__c</errorDisplayField>
        <errorMessage>You don&apos;t have access to request any amendment request except Time Extension</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_End_Date_Validation</fullName>
        <active>true</active>
        <description>Please provide &quot;Time Extension Start Date&quot; and &quot;Time Extension End Date&quot;.</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),AND( OR(ISPICKVAL( AmendmentType__c, &apos;Time Extension&apos;), ISPICKVAL( AmendmentType__c , &apos;Additional Funding and Time Extension&apos;)) ,  OR(ISBLANK(  TimeExtensionStartDate__c ), ISBLANK( TimeExtensionEndDate__c ))   ))</errorConditionFormula>
        <errorMessage>Please provide &quot;Time Extension Start Date&quot; and &quot;Time Extension End Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>State_Federal_Cost_Share_validation</fullName>
        <active>true</active>
        <description>Please provide &quot;State Share%&quot; and &quot;FEMA Share%&quot;.</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),AND(OR(ISPICKVAL( AmendmentType__c, &apos;FEMA Percentage Change&apos;), ISPICKVAL( AmendmentType__c , &apos;Additional Funding and FEMA Percentage Change&apos;)),  OR(ISBLANK(  State_Share__c ),ISBLANK(   FEMA_Share__c ))  ))</errorConditionFormula>
        <errorMessage>Please provide &quot;State Share%&quot; and &quot;FEMA Share%&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Suuplement_Award_Validation</fullName>
        <active>true</active>
        <description>When the &quot;Amendment Type&quot; is &quot;Money Extension&quot;, We need to select the &quot;Supplement Award&quot;.</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),AND( OR(ISPICKVAL( AmendmentType__c , &apos;Money Extension&apos;), ISPICKVAL( AmendmentType__c , &apos;Additional Funding and FEMA Percentage Change&apos;), ISPICKVAL( AmendmentType__c , &apos;Additional Funding and Time Extension&apos;))  , ISBLANK(  SupplementAward__c )))</errorConditionFormula>
        <errorDisplayField>SupplementAward__c</errorDisplayField>
        <errorMessage>Please provide &quot;Supplement Award&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Extension_End_Date_Validation</fullName>
        <active>false</active>
        <description>Time Extension End Date is mandatory when Amendment Type is Time Extension</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),AND( OR(ISPICKVAL( AmendmentType__c, &apos;Time Extension&apos;), ISPICKVAL( AmendmentType__c , &apos;Additional Funding and Time Extension&apos;)) ,ISBLANK( TimeExtensionEndDate__c ) ))</errorConditionFormula>
        <errorDisplayField>TimeExtensionEndDate__c</errorDisplayField>
        <errorMessage>Please provide &quot;Time Extension End Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Extension_Start_Date_And_End_Date</fullName>
        <active>true</active>
        <description>Time Extension End Date should be greater than Time Extension Start Date</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),TimeExtensionEndDate__c &lt; TimeExtensionStartDate__c)</errorConditionFormula>
        <errorDisplayField>TimeExtensionEndDate__c</errorDisplayField>
        <errorMessage>&quot;Time Extension End Date&quot; should be greater than &quot;Time Extension Start Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Extension_Start_Date_Validation</fullName>
        <active>false</active>
        <description>Time Extension Start Date is mandatory when Amendment Type is Time Extension</description>
        <errorConditionFormula>AND(ISBLANK(External_Id__c ),AND(OR(ISPICKVAL( AmendmentType__c, &apos;Time Extension&apos;), ISPICKVAL( AmendmentType__c , &apos;Additional Funding and Time Extension&apos;)),ISBLANK( TimeExtensionStartDate__c )))</errorConditionFormula>
        <errorDisplayField>TimeExtensionStartDate__c</errorDisplayField>
        <errorMessage>Please provide &quot;Time Extension Start Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Extension_Validation</fullName>
        <active>true</active>
        <description>Supplement Award, FEMA Share%, and State Share% are not applicable for the &quot;Time Extension&quot; Amendment.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) , ISPICKVAL( AmendmentType__c , &apos;Time Extension&apos;) , OR(  AND( NOT( ISBLANK(  SupplementAward__c ) ) ,SupplementAward__c != 0) , AND(  NOT( ISBLANK(FEMA_Share__c) ) ,FEMA_Share__c != 0) ,AND(  NOT( ISBLANK( State_Share__c ) ),State_Share__c != 0)))</errorConditionFormula>
        <errorMessage>Supplement Award, FEMA Share%, and State Share% are not applicable for the &quot;Time Extension&quot; Amendment.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
