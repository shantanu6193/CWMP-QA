    <validationRules>
        <fullName>Active_Award_Validation</fullName>
        <active>true</active>
        <description>You can only select the Award which is associated with the same Project and in &quot;Active&quot; Stage.</description>
        <errorConditionFormula>AND( NOT( ISPICKVAL( Award__r.Status__c , &apos;Active&apos;) ) ,  OR(ISPICKVAL( Stage__c  , &apos;New&apos;),ISPICKVAL( Stage__c  , &apos;Processing&apos;), ISPICKVAL( Stage__c  , &apos;Reviewed - Needs Information&apos;)) )</errorConditionFormula>
        <errorMessage>You can only select the Award which is associated with the same Project and in &quot;Active&quot; Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Based_on_Payment_Type</fullName>
        <active>true</active>
        <description>Payment Type. If Standard, Payment amount(Expenditure to Date) must be greater than 0. If Reimbursements, payment amount(Expenditure to Date)  must be less than 0</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(NOT( OR( ISPICKVAL( Stage__c , &apos;New&apos;), ISPICKVAL( Stage__c , &apos;Processing&apos;), ISPICKVAL( Stage__c , &apos;Reviewed - Needs Information&apos;), ISPICKVAL( Stage__c , &apos;Withdrawn&apos;))  ), OR(AND( ISPICKVAL( PaymentType__c , &apos;Reimbursement&apos;) ,    TotalApprovedPaymentAmount__c &lt; 0),
AND( ISPICKVAL( PaymentType__c , &apos;DeObligation&apos;) ,  TotalApprovedPaymentAmount__c &gt; 0))))</errorConditionFormula>
        <errorDisplayField>TotalApprovedPaymentAmount__c</errorDisplayField>
        <errorMessage>&quot;Total Approved Payment Amount&quot; must be Greater than 0 for &quot;Reimbursement&quot;  Payment Type and must be less than 0 for &quot;De Obligation&quot; Payment Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approved_Federal_Amount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND( NOT( OR(ISPICKVAL( PaymentType__c , &apos;Offset&apos;), ISPICKVAL( PaymentType__c , &apos;Returned Project Payment&apos;),  AND(ISPICKVAL( Project__r.Stage__c , &apos;Closeout&apos;),ISPICKVAL( PaymentType__c , &apos;Final Payment&apos;) ) ) ) ,
    ApprovedFederalPaymentAmount__c &gt; CEILING(FederalEstimatedPaymentAmount__c), ApprovedFederalPaymentAmount__c != 0
))</errorConditionFormula>
        <errorDisplayField>ApprovedFederalPaymentAmount__c</errorDisplayField>
        <errorMessage>&quot;Approved Federal Payment Amount&quot; must not be greater than &quot;Estimated Federal Payment Amount&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approved_State_Payment_Amount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND( NOT(  OR(ISPICKVAL( PaymentType__c , &apos;Offset&apos;), ISPICKVAL( PaymentType__c , &apos;Returned Project Payment&apos;),  AND(ISPICKVAL( Project__r.Stage__c , &apos;Closeout&apos;),ISPICKVAL( PaymentType__c , &apos;Final Payment&apos;) ) ) ) ,
     ApprovedStatePaymentAmount__c &gt; CEILING(StateEstimatedPaymentAmount__c), ApprovedStatePaymentAmount__c != 0
))</errorConditionFormula>
        <errorDisplayField>ApprovedStatePaymentAmount__c</errorDisplayField>
        <errorMessage>&quot;Approved State Payment Amount&quot; must not be greater than &quot;Estimated State Payment Amount&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Award_Field_Validation</fullName>
        <active>true</active>
        <description>Award is mandatory at any stage of Payment Request.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,ISBLANK( Award__c ))</errorConditionFormula>
        <errorDisplayField>Award__c</errorDisplayField>
        <errorMessage>Please select an  active Award.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expenditure_to_date_NA_Validation</fullName>
        <active>true</active>
        <description>Expenditure to Date is not applicable for &quot;Offset/Returned Project Payment&quot; Validation.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) , OR(ISPICKVAL( PaymentType__c ,&apos;Offset&apos;),ISPICKVAL(PaymentType__c ,&apos;Returned Project Payment&apos;)),   AND(  NOT(ISBLANK( ExpenditurestoDate__c ))  ,ExpenditurestoDate__c 
 != 0) )</errorConditionFormula>
        <errorDisplayField>ExpenditurestoDate__c</errorDisplayField>
        <errorMessage>&quot;Expenditure to Date&quot; is not applicable for &quot;Offset/Returned Project Payment&quot; type Payment Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expenditures_to_Date_Validation</fullName>
        <active>true</active>
        <description>Expenditures to Date is mandatory and must not be zero when Stage is Processing</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(
OR(ISPICKVAL(Stage__c,&apos;Processing&apos;),ISPICKVAL(Stage__c,&apos;Payment Processing&apos;),ISPICKVAL(Stage__c,&apos;Paid&apos;),ISPICKVAL(Stage__c,&apos;In Review&apos;)), 
ISBLANK(ExpenditurestoDate__c)
))</errorConditionFormula>
        <errorDisplayField>ExpenditurestoDate__c</errorDisplayField>
        <errorMessage>Please provide &quot;Expenditures to Date&quot; for Payment Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manager_Approver_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(
  ISBlank( $User.ManagerId ) ,
  ($Profile.Name &lt;&gt; &apos;System Administrator&apos;)
))</errorConditionFormula>
        <errorMessage>Your user record does not seem to have a manager assigned. Please contact your administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Obligated_Agency_Validation</fullName>
        <active>true</active>
        <description>You must select the &quot;Obligated Agency&quot;, if the Payment Type is &quot;Mutual Aid&quot;.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND( ISPICKVAL( PaymentType__c , &apos;Mutual Aid&apos;) , ISBLANK( ObligatedAgency__c ) ))</errorConditionFormula>
        <errorDisplayField>ObligatedAgency__c</errorDisplayField>
        <errorMessage>You must select the &quot;Obligated Agency&quot; if the Payment Type is &quot;Mutual Aid&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Offset_Payment_Validation</fullName>
        <active>true</active>
        <description>Expenditure do date must be negative amount, when Payment Type is &quot;Offset&quot;.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND( ISPICKVAL( PaymentType__c , &apos;Offset&apos;) ,  Project__r.AmountAvailable__c &lt; 0, ExpenditurestoDate__c &gt;0))</errorConditionFormula>
        <errorDisplayField>ExpenditurestoDate__c</errorDisplayField>
        <errorMessage>&quot;Expenditures To Date&quot; must be a negative amount when Payment Type is &quot;Offset&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PR_Withdrawal_Reason</fullName>
        <active>true</active>
        <description>&quot;Reason To Withdraw&quot; is required to mark the Payment Request as &quot;Withdrawn&quot;.</description>
        <errorConditionFormula>AND( ISCHANGED( Stage__c ) , ISPICKVAL( Stage__c , &apos;Withdrawn&apos;) , ISBLANK( Reason_To_Withdraw__c ) )</errorConditionFormula>
        <errorDisplayField>Reason_To_Withdraw__c</errorDisplayField>
        <errorMessage>&quot;Reason To Withdraw&quot; is required to mark the Payment Request as &quot;Withdrawn&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Delivery_Validation</fullName>
        <active>true</active>
        <description>Payment Delivery is mandatory when Stage is Processing</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(
OR(ISPICKVAL(Stage__c,&apos;Processing&apos;),ISPICKVAL(Stage__c,&apos;Payment Processing&apos;),ISPICKVAL(Stage__c,&apos;Paid&apos;),ISPICKVAL(Stage__c,&apos;In Review&apos;)),   NOT( OR(ISPICKVAL( PaymentType__c ,&apos;Offset&apos;),ISPICKVAL(PaymentType__c ,&apos;Returned Project Payment&apos;))),  
ISBLANK(TEXT(PaymentDelivery__c))))</errorConditionFormula>
        <errorDisplayField>PaymentDelivery__c</errorDisplayField>
        <errorMessage>Please provide &quot;Payment Delivery&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Type_Validation</fullName>
        <active>true</active>
        <description>Payment Type is mandatory when Stage is Processing</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(
OR(ISPICKVAL(Stage__c,&apos;Processing&apos;),
ISPICKVAL(Stage__c,&apos;In Review&apos;),
ISPICKVAL(Stage__c,&apos;Payment Processing&apos;),
ISPICKVAL(Stage__c,&apos;Paid&apos;)),
ISPICKVAL( PaymentType__c ,&quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>PaymentType__c</errorDisplayField>
        <errorMessage>Please provide &quot;Payment Type&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reimbursement_End_Date_Validation</fullName>
        <active>true</active>
        <description>Reimbursement End Date is mandatory when Stage is Processing</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(OR(ISPICKVAL(Stage__c,&apos;Processing&apos;),ISPICKVAL(Stage__c,&apos;Payment Processing&apos;),ISPICKVAL(Stage__c,&apos;Paid&apos;),ISPICKVAL(Stage__c,&apos;In Review&apos;)) ,
NOT( CONTAINS(  Project__r.Program_Name__r.Parent_Program__r.Master_Label_Program_Name__c  , &apos;Public Assistance&apos;) ),
ISPICKVAL( PaymentType__c , &apos;Reimbursement&apos;),
ISBLANK( ReimbursementEndDate__c )
))</errorConditionFormula>
        <errorDisplayField>ReimbursementEndDate__c</errorDisplayField>
        <errorMessage>Please provide &quot;Reimbursement End Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reimbursement_Start_Date_Validation</fullName>
        <active>true</active>
        <description>Reimbursement Start Date is mandatory when Stage is Processing</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND(OR(ISPICKVAL(Stage__c,&apos;Processing&apos;),ISPICKVAL(Stage__c,&apos;Payment Processing&apos;),ISPICKVAL(Stage__c,&apos;Paid&apos;),ISPICKVAL(Stage__c,&apos;In Review&apos;)),
NOT( CONTAINS( Project__r.Program_Name__r.Parent_Program__r.Master_Label_Program_Name__c , &apos;Public Assistance&apos;) ),
ISPICKVAL( PaymentType__c , &apos;Reimbursement&apos;),
ISBLANK(ReimbursementStartDate__c)
))</errorConditionFormula>
        <errorDisplayField>ReimbursementStartDate__c</errorDisplayField>
        <errorMessage>Please provide &quot;Reimbursement Start Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reimbursment_Start_Date_And_End_Date</fullName>
        <active>true</active>
        <description>Reimbursment end date should be greater than start date</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,ReimbursementEndDate__c &lt;  ReimbursementStartDate__c)</errorConditionFormula>
        <errorDisplayField>ReimbursementEndDate__c</errorDisplayField>
        <errorMessage>&quot;Reimbursement End Date&quot; should be greater than &quot;Reimbursement Start Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Returned_Project_Payment_Validation</fullName>
        <active>true</active>
        <description>Approved State Payment Amount/Approved Federal payment Amount cannot be greater than 0 for the &quot;Returned Project Payment&quot; Payment Request.</description>
        <errorConditionFormula>AND( ISPICKVAL( PaymentType__c , &apos;Returned Project Payment&apos;), OR( ApprovedFederalPaymentAmount__c &gt; 0 , ApprovedStatePaymentAmount__c &gt; 0) )</errorConditionFormula>
        <errorMessage>Approved State Payment Amount/Approved Federal Payment Amount cannot be greater than 0 for the &quot;Returned Project Payment&quot; Payment Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAM_Registration_Date_Validation</fullName>
        <active>true</active>
        <description>This Validation is used to prevent payment when Ob ligating Sub Recipient has expired SAM Registration date.</description>
        <errorConditionFormula>AND( ISBLANK( External_Id__c ) ,AND( ISNEW(),Project__r.Obligating_Sub_Recipient__r.DUNS_Expiration_Date__c &lt; TODAY())
)</errorConditionFormula>
        <errorMessage>Your Obligating Sub Recipient&apos;s &quot;SAM Registration Expiration Date&quot; has lapsed.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
