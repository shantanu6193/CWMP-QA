<aura:component description="CustomFieldViewForList">
    <aura:attribute name="fieldLabel" type="String"/>
    <aura:attribute name="orderItem" type="SObject"/>
    <aura:attribute name="apiName" type="String"/>
    <aura:attribute name="value" type="String"/>
    <aura:attribute name="configList" type="List" default="[]"/>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <div class="slds-truncate">{!v.value}</div>

</aura:component>